  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7b8ac7",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#7dcfff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.control",
        "keyword.operator.logical",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#7dcfff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.bitwise",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#88C0D0"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#B48EAD",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": "constant.language.null",
      "settings": {
        "foreground": "#BF616A",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.language.undefined",
      "settings": {
        "foreground": "#BF616A",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#EBCB8B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#A3BE8C"
      }
    },
    {
      "scope": [
        "string.quoted.single",
        "string.quoted.double",
        "string.template"
      ],
      "settings": {
        "foreground": "#A3BE8C"
      }
    },
    {
      "scope": [
        "string.regexp",
        "constant.regexp"
      ],
      "settings": {
        "foreground": "#EBCB8B"
      }
    },
    {
      "scope": [
        "constant.character",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#B48EAD"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other.readwrite",
        "variable.parameter",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#e4f0fb"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#8FBCBB"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#7dcfff",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.namespace",
        "entity.name.type.namespace"
      ],
      "settings": {
        "foreground": "#EBCB8B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.macro",
        "entity.name.other.preprocessor.macro"
      ],
      "settings": {
        "foreground": "#BF616A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.decimal",
        "constant.numeric.hex"
      ],
      "settings": {
        "foreground": "#B48EAD"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#8FBCBB",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#8FBCBB"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#7b8ac7"
      }
    },
    {
      "scope": [
        "punctuation.section",
        "punctuation.brackets",
        "punctuation.parenthesis"
      ],
      "settings": {
        "foreground": "#88C0D0"
      }
    },
    {
      "scope": [
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#EBCB8B",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#88C0D0"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#81A1C1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#8FBCBB"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#EBCB8B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#e4f0fb",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.bold",
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#e4f0fb",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.quote",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#A3BE8C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#88C0D0",
        "fontStyle": "underline"
      }
    }
  ]
}
